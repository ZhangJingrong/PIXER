### SUM the four branches
layer {
  bottom: "fc8_${EXP}_1"
  bottom: "fc8_${EXP}_2"
  bottom: "fc8_${EXP}_3"
  bottom: "fc8_${EXP}_4"
  top: "fc8_${EXP}"
  name: "fc8_${EXP}"
  type: "Eltwise"
  eltwise_param {
    operation: SUM
  }
}
## original resolution
layer {
  name: "fc8_interp"
  type: "Interp"
  bottom: "fc8_${EXP}"
  top: "fc8_interp"
  interp_param {
    zoom_factor: 12
  }
}
